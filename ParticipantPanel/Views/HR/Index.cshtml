@model HomeVM


 <div class="card shadow mb-4">
                        <div class="card-header py-3">
                            <h6 class="m-0 font-weight-bold text-primary"><a href="/hr/create">Create</a></h6>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Surname</th>
                                            <th>Age</th>
                                            <th>Gender</th>
                                            <td>Faculty</td>
                                            <th>University</th>
                                            <th>Department</th>
                                           
                                            <th></th>
                                            
                                        </tr>
                                    </thead>
                                 
                                    <tbody>
                                            
                                            @foreach (var item in Model.humen)
                                             {
                                                <tr>
                                            <td>@item.Name</td>
                                            <td>@item.Surname</td>
                                             <td>
                                                @item.Age
                                            </td>
                                            <td>@item.Gender</td>
                                            <td>@item.Faculty</td>
                                           
                                            
                                            <td>
                                                @item.University

                                            </td>
                                            <td>@item.Department</td>
                                            <td>  
                                             <a href="hr/delete/@item.Id">
                                            <button class="btn btn-danger" >Delete</button>
                                            </a>
                                            </td>
                                               </tr>
                                            }
                                        
                                 
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
